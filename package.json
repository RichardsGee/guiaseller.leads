{
  "name": "guiaseller-leads",
  "version": "1.0.0",
  "description": "GuiaSeller Leads - Internal lead intelligence platform with AIOS Discord bots",
  "main": "src/index.js",
  "type": "module",
  "scripts": {
    "dev": "node src/index.js",
    "dev:api": "cd backend && npm run dev",
    "dev:web": "cd frontend && npm run dev",
    "dev:all": "npm run dev:api & npm run dev:web",
    "build:api": "cd backend && npm run build",
    "build:web": "cd frontend && npm run build",
    "build": "npm run build:api && npm run build:web",
    "lint:api": "cd backend && npm run lint",
    "lint:web": "cd frontend && npm run lint",
    "typecheck:api": "cd backend && npx tsc --noEmit",
    "typecheck:web": "cd frontend && npx tsc --noEmit",
    "typecheck": "npm run typecheck:api && npm run typecheck:web",
    "db:generate": "cd backend && npx prisma generate",
    "db:migrate": "cd backend && npx prisma migrate dev",
    "db:push": "cd backend && npx prisma db push",
    "db:studio": "cd backend && npx prisma studio",
    "db:seed": "cd backend && npx tsx prisma/seed.ts",
    "start": "node src/index.js",
    "server": "node src/server.js",
    "bots:start": "node src/bots/index.js",
    "bot:dex": "node src/bots/dex-bot.js",
    "bot:test": "node scripts/test-discord-bot.js",
    "webhook:test": "node scripts/test-webhook.js",
    "test": "jest",
    "lint": "eslint src/"
  },
  "keywords": [
    "guiaseller",
    "leads",
    "discord",
    "aios",
    "agents"
  ],
  "author": "GuiaSeller Team",
  "license": "MIT",
  "dependencies": {
    "axios": "^1.13.5",
    "discord.js": "^14.14.0",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "firebase-admin": "^12.0.0",
    "prisma": "^5.7.1"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^20.10.6",
    "eslint": "^8.55.0",
    "jest": "^29.7.0",
    "typescript": "^5.3.3"
  }
}
